server {
    listen 80;


    server_name 127.0.0.1:8000;
    root /home/test-server/wooman-test;

    keepalive_timeout 5;
    client_max_body_size 4G;

    access_log /home/test-server/wooman-test/logs/nginx-access.log;
    error_log /home/test-server/wooman-test/logs/nginx-error.log;

    location /api/v1/login.html {
        root /home/test-server/wooman-test/FRONTEND;
        access_log  on;
    }

    location /api/v1/create-password.html {
        root /home/test-server/wooman-test/FRONTEND;
        access_log  on;
    }
